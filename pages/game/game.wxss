/* pages/anim/anim.wxss */
page {
  height: 100%;
}
.bg {
  width: 750rpx;
  height: 100%;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size: 100% auto;
  justify-content: center;
  text-align: center;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 2;
}
button {
  z-index: 1;
}

.black-bg-cover {
  width: 750rpx;
  height: 1200rpx;
  background: #000;
  opacity: 0.7;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}

.chart-group text.show {
  width: 100%;
  height: 100%;
  position: absolute; 
  top: 0;
  z-index: 1;
  display: block;
  background-color: #fcf4dc; 
}

.chart.hide {
  display: none;
}

.chart-group {
  width: 640rpx;
  height: auto;
  position: absolute;
  top: -190rpx;
  left: 0;
  z-index: 3;
  margin: 0 55rpx;
}

.chart-group text.dog-bg {
    background: url('data:image/png;base64,') no-repeat center #fcf4dc;
}
.chart {
  width: 75px;
  height: 75px;
  display: flex;
  float: left;
  flex-direction: row;
  transform: scale(0.01);
  margin: 2px;
  background: none;
  color: #e62f2d;
  justify-content:center;
  line-height: 75px;
  font-size: 70rpx;
  transform-style: preserve-3d;
  position: absolute;
}
.l200 {
  left: 200px;
}

.t200 {
  top: 200px;
}

.dog-bg-base {
  width: 100%;
  height: 100%;
  position: absolute; 
  top: 0;
  background: url('data:image/png;base64,') no-repeat center #fcf4dc;
  backface-visibility: hidden;
  z-index: 0;
  transform: rotateY(-180);
}

.back {
  width: 100%;
  height: 100%;
  position: absolute; 
  top: 0;
  background: yellow;
  backface-visibility: hidden;
}

.chart1 {
  top: -100rpx;
  left: 150rpx;
}

.chart2 {
  top: -100rpx;
  left: 270rpx;
}

.chart3 {
  top: -100rpx;
  left: 390rpx;
}

.chart4 {
  top: -100rpx;
  left: 510rpx;
}

.chart-img-1 {
  width: 100%;
  height: 100%;
  float: left;
}

.card-group {
  width: 640rpx;
  height: 100%;
  position: absolute;
  top: 300rpx;
  left: 55rpx;
  z-index: 1;
}

.card {
  width: 75px;
  height: 75px;
  background: url('data:image/png;base64,') no-repeat center #fcf4dc;
  display: flex;
  flex-direction: row;
  margin: 2px;
  float: left;
  transform: scale(0.01);
}

.coupon-box {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 6;
}

.card-group.hide {
  display: none;
}
.card-group.show {
  display: block;
}

.card-group .card.hide {
  opacity: 0.1;
}

.bottom-tips {
  width: 750rpx;
  height: 220rpx;
  color: #fcf4dc;
  font-size: 40rpx;
  text-align: center;
  justify-content: center;
  position: absolute;
  bottom: 0;
  z-index: 5;
  background: none;
}

.bottom-tips .bottom-btns {
  width: 750rpx;
  height: 80rpx;
  line-height: 70rpx;
  margin-top: 50rpx;
  font-size: 30rpx;
}

.again-btn {
  margin-right: 100rpx;
}

.top-tips-box {
  width: 750rpx;
  height: 150rpx;
  color: #fcf4dc;
  font-size: 40rpx;
  text-align: center;
  justify-content: center;
  position: absolute;
  top: 20rpx;
  z-index: 5;
  background: none;
}

.top-tips {
  width: 750rpx;
  height: 60rpx;
  line-height: 60rpx;
  justify-content: center;
  text-align: center
}
.color1 {
  color: #ffff00;
  font-size: 50rpx;
}

.coupon-panel {
  width: 500rpx;
  height: auto;
  position: absolute;
  top: 200rpx;
  left: 125rpx;
  background: #cd523d;
  border-radius: 30rpx;
  z-index: 6;
  justify-content: center;
  text-align: center;
  color: #fcf4dc;
  font-size: 24rpx;
  overflow: hidden;
}

image.u-img {
  width: 120rpx;
  height: 120rpx;
  background: #fcf4dc;
  border-radius: 60rpx;
  margin-top: 110rpx;
  position: relative;
  z-index: 2;
}

.u-name {
  width: 100%;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 32rpx;
  display: block;
  margin-top: 20rpx;
}

.greeting-words {
  width: 100%;
  margin-top: 20rpx;
  display: block;
}

.get-money {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  margin-top: 70rpx;
  margin-bottom: 70rpx;
}

.get-money text.u-money {
  font-size: 50rpx;
}

.creat-one-btn {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  margin-bottom: 50rpx;
  display: block;
}

.coupon-bg {
  width: 300%;
  height: 300%;
  display: block;
  background: #d85840;
  border-radius: 150%;
  z-index: 0;
  position: absolute;
  top: -273%;
  left: -100%;
  box-shadow: 0 0 10rpx #902d1a;
}

.close-coupon-panel-btn {
  width: 50rpx;
  height: 50rpx;
  line-height: 46rpx;
  display: block;
  background: #fcf4dc;
  color: #e62f2d;
  font-size:30rpx;
  position: absolute;
  top: 150rpx;
  left: 600rpx;
  border-radius: 25rpx;
  justify-content: center;
  text-align: center;
}